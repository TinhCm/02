#include<iostream>	//BAI02: Tinh gia tri tuyet doi cua so nguyen.
using namespace std;
int main()
{
	int n;
	cout << "nhap vap so nguyen n: ";
	cin >> n;
	if (n < 0)
		cout << "Gia tri tuyet doi cua so nguyen " << n << " la: " << n * -1 << endl;
	else
		cout << "Gia tri tuyet doi cua so nguyen " << n << " la: " << n << endl;
	system("pause");
	return 0;


}
