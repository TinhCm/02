#include<iostream>
using namespace  std;
int main()
{
	int a, b;
	cout << "Nhap vap chieu dai a: ";
	cin >> a;
	cout << "Nhap vao chieu rong b: ";
	cin >> b;

	for (int i = 1; i <= b; i++)
	{
		for (int j = 1; j <= a; j++)
		{
			if (i == 1 || j==1 || i == b || j == a)
			{
				cout << "*";
			}
			else
			{
				cout << " ";
			}
		}
		cout <<" "<< endl;
	}
	system("pause");
	return 0;
}
