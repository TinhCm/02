#include<iostream>	\\Bai03: Nhap vao 3 so nguyen, xuat ra thu tu tang dan cua cac so.
using namespace std;
int main()
{
	int a, b, c,bienPhu;
	cout << "Nhap vao so nguyen a: ";
	cin >> a;
	cout << "Nhap vao so nguyen b: ";
	cin >> b;
	cout << "Nhap vao so nguyen c: ";
	cin >> c;
	if (a > b)
	{
		bienPhu = a;
		a = b;
		b = bienPhu;
	}
	if (a > c)
	{
		bienPhu = a;
		a = c;
		c = bienPhu;
	}
	if (b > c)
	{
		bienPhu = b;
		b = c;
		c = bienPhu;
	}
		cout << "Gia tri cua cac so theo thu tu tang dan la: " << a<<" --> "<< b<<" --> "<< c<<"."<<endl;
		system("pause");
		return 0;

}
